\pard\plain \fi720\qj\hyphpar1\sb0\sa0 \f4\fs24 {\*\bkmkstart sub_3302}\cs65552\f4\fs24 ПП{\sub \~i}=СЧП{\sub \~i}{\pict\picw150\pich285 \picwgoal176\pichgoal334 \wmetafile8 }15,\par\pard
